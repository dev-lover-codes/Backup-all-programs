                   print(f\"Player {current_player} wins!\")\n","                    game_over = True\n","                elif check_draw(board):\n","                    print_board(board)\n","                    print(\"It's a draw!\")\n","                    game_over = True\n","                else:\n","                    current_player = \"O\" if current_player == \"X\" else \"X\"\n","            else:\n","                print(\"Invalid move. Please choose an empty spot between 1 and 9.\")\n","        except ValueError:\n","            print(\"Invalid input. Please enter a number.\")\n","        except EOFError: # Handles case where input stream closes in some environments\n","            print(\"Game ended unexpectedly.\")\n","            game_over = True\n","\n","    if input(\"Play again? (yes/no): \").lower() == 'yes':\n","        play_game()\n","    else:\n","        print(\"Thanks for playing!\")\n","\n","# Start the game\n","play_game()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"collapsed":true,"id":"-XB28O-UpoI2","outputId":"3c045cb9-abb2-47c5-b9d5-71fc8d2885b2"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Welcome to Text-Based Tic Tac Toe!\n","Enter a number from 1-9 to place your mark.\n","  |   |  \n","---------\n","  |   |  \n","---------\n","  |   |  \n","Player X, enter your move (1-9): 1\n","X |   |  \n","---------\n","  |   |  \n","---------\n","  |   |  \n","Player O, enter your move (1-9): 5\n","X |   |  \n","---------\n","  | O |  \n","---------\n","  |   |  \n","Player X, enter your move (1-9): 3\n","X |   | X\n","---------\n","  | O |  \n","---------\n","  |   |  \n","Player O, enter your move (1-9): 6\n","X |   | X\n","---------\n","  | O | O\n","---------\n","  |   |  \n","Player X, enter your move (1-9): 2\n","X | X | X\n","---------\n","  | O | O\n","---------\n","  |   |  \n","Player X wins!\n","Play again? (yes/no): no\n","Thanks for playing!\n"]}]},{"cell_type":"markdown","so
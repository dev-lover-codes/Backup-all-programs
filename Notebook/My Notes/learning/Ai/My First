is an advanced AI voice generator running in Google Colab.\"\n","\n","# ==========================\n","# 3️⃣ Generate Voice with OpenAI TTS\n","# ==========================\n","def generate_openai_tts(text, filename=\"openai_output.mp3\"):\n","    if not openai.api_key:\n","        print(\"OpenAI API key is not set. Skipping OpenAI TTS generation.\")\n","        return\n","    try:\n","        response = openai.audio.speech.create(\n","            model=\"gpt-4o-mini-tts\",\n","            voice=\"alloy\",  # you can change the voice\n","            input=text\n","        )\n","        response.stream_to_file(filename) # Use stream_to_file method\n","        display(Audio(filename, autoplay=True))\n","        print(f\"✅ OpenAI TTS saved to {filename}\")\n","    except openai.AuthenticationError as e:\n","        print(f\"OpenAI Authentication Error: {e}\")\n","    except Exception as e:\n","        print(f\"An error occurred during OpenAI TTS generation: {e}\")\n","\n","generate_openai_tts(text_to_speak)\n","\n","# ==========================\n","# 4️⃣ Generate Voice with ElevenLabs\n","# ==========================\n","def generate_elevenlabs_tts(text, filename=\"elevenlabs_output.mp3\", voice=\"Rachel\"):\n","    if not elevenlabs_client:\n","        print(\"ElevenLabs API client is not initialized. Skipping ElevenLabs TTS generation.\")\n","        return\n","    try:\n","        # Use client methods for generate and save\n","        audio = elevenlabs_client.generate(text=text, voice=voice, model=\"eleven_multilingual_v2\")\n","        elevenlabs_client.save(audio, filename)\n","        display(Audio(filename, autoplay=True))\n","        print(f\"✅ ElevenLabs TTS saved to {filename}\")\n","    except Exception as e:\n","        print(f\"An error occurred during ElevenLabs TTS generation: {e}\")\n","\n","generate_elevenlabs_tts(text_to_speak)\n"],"metadata":{"collapsed":true,"colab":{"base_uri":"https://localhost:8080/"},"id":"cAMBsJUmzMnF","executionInfo":{"status